        "collision_type": "push",

        "movement_behavior_params": {},
        "movement_behavior_movement_type": "turret"
      },
      "components": {
        "moving:Position": {
          "hook": "enemy_pos_1",
          "pos": {
            "x": 0.0,
            "y": 0.0
          }
        },
        "ui:Drawable": {},
        "ui:AnimatedSprite": {
          "animations": [
            {
              "name": "idle",
              "texture": "$idle_texture",
              "frame_size": "$idle_frame_size",
              "frame_pos": "$idle_frame_pos",
              "direction": "$idle_direction",
              "sprite_size": "$idle_sprite_size",
              "framerate": 10.0,
              "nb_frames": "$idle_nb_frames",
              "loop": true,
              "rollback": true
            },
            {
              "name": "death",
              "texture": "$death_texture",
              "frame_size": "$death_frame_size",
              "frame_pos": "$death_frame_pos",
              "direction": "$death_direction",
              "sprite_size": "$death_sprite_size",
              "framerate": 10.0,
