{
    "entities_template": [
        {
            "name": "link",
            "default_parameters": {
                "width": 0.5,
                "height": 0.1,
                "x": 0.0,
                "y": 0.0,
                "to_emit": []
            },
            "components": {
                "ui:Drawable": {},
                "moving:Position": {
                    "pos": {
                        "x": "$x",
                        "y": "$y"
                    }
                },
                "ui:Text": {
                    "font": "assets/fonts/roboto.ttf",
                    "size": {
                        "width": "$width",
                        "height": "$height"
                    },
                    "fill_color": {
                        "r": 255,
                        "g": 255,
                        "b": 255,
                        "a": 255
                    },
                    "outline_color": {
                        "r": 0,
                        "g": 0,
                        "b": 0,
                        "a": 255
                    },
                    "outline_thickness": 30.0,
                    "outline": true,
                    "text": "$text"
                },
                "collision:Collidable": {
                    "size": {
                            "x": "$width",
                            "y": "$height"
                        },
                    "collision_type": "Trigger"
                },
                "ath:Clickable": {
                    "to_emit": "$to_emit"
                }
            }
        }
    ],
    "scenes": []
}