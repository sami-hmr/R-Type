{
    "entities_template": [
        {
            "name": "text",
            "default_parameters": {
                "width": 0.5,
                "height": 0.1,
                "x": 0.0,
                "y": 0.0,
                "z": 1,
                "font": "assets/fonts/roboto.ttf",
                "fill_color": {
                    "r": 255,
                    "g": 255,
                    "b": 255,
                    "a": 255
                },
                "outline_color": {
                    "r": 0,
                    "g": 0,
                    "b": 0,
                    "a": 255
                },
                "outline_thickness": 30.0,
                "outline": true
            },
            "components": {
                "ui:Drawable": {},
                "moving:Position": {
                    "pos": {
                        "x": "$x",
                        "y": "$y"
                    },
                    "z": "$z"
                },
                "ui:Text": {
                    "font": "$font",
                    "size": {
                        "width": "$width",
                        "height": "$height"
                    },
                    "fill_color": "$fill_color",
                    "outline_color": "$outline_color",
                    "outline_thickness": 30.0,
                    "outline": true,
                    "text": "$text"
                }
            }
        }
    ],
    "scenes": []
}